CREATE TABLE public.ACCOUNTS
(
  ACCOUNTID NUMERIC PRIMARY KEY NOT NULL,
  ACCOUNTTYPE VARCHAR(50) NOT NULL,
  BANK VARCHAR(50) NOT NULL,
  ADRESS VARCHAR(50) NOT NULL,
  ACCOUNTNR VARCHAR(50) NOT NULL,
  SWIFT VARCHAR(50) NOT NULL,
  IBAN VARCHAR(255),
  TEL VARCHAR(20),
  BALLANCE NUMERIC NOT NULL,
  OFFICEID NUMERIC NOT NULL
);
CREATE SEQUENCE ACCOUNTID_SEQ MINVALUE 100 MAXVALUE 10000000000 START WITH 101 INCREMENT BY 1;